<!DOCTYPE html>
<html>
<head>
        <title> Annual Events </title>
        <meta charset="utf-8" />
        <meta name="viewpoint" content="width=device-width intial-scale=1 shrink-to-fit=no" />
        <link rel="icon" href="fav.ico" />
        <link rel="stylesheet" href="/css/style.css">
        <link rel="stylesheet" type="text/css" href="css/annual.css">
        <!-- <script src="//code.jquery.com/jquery-3.2.1.min.js"></script> -->
        <!-- <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.0.47/jquery.fancybox.min.css" /> -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" />
        <link href="https://fonts.googleapis.com/css2?family=Baloo+Bhai+2:wght@600&family=Overpass:wght@300&family=Roboto+Mono:wght@300&family=Source+Sans+Pro:wght@200&family=Ubuntu&display=swap" rel="stylesheet">

        <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.0.47/jquery.fancybox.min.js"></script> -->
        <!-- <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script> -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<body>
    
    <%- include('header'); -%>
    <!-- NAVIGATION BAR -->
    <%- include('navbar'); -%>



    <div class="container" style="text-align-last: center;">
        <h1>Annual Events/ Activities</h1>

        <h3>CDAC in North East (CNE)</h3>

        <div class="row">
          <% event.forEach(function(event) { %>
            <div class="col-md-4">
                <div class="thumbnail">
                    <a data-fancybox="video-gallery" href="<%= event.detaillink %>" target="_blank" ><img src="../event/<%= event.link %>"></a>
                    <h4 style="text-align:center ;"><%= event.username %></h4>
                </div>
            </div>
          <% }) %>
            
        </div>
    </div>






    <%- include('footer'); -%>

    <!-- SCRIPT LINK -->
<script src="/js/script.js"></script>

