<body>

	<div class="wrapper">
		<div>
			<!-- Sidebar -->
			<%- include('sidebar'); -%>
		</div>
	</div>
	<div style="margin-left:250px;">
		<%- include('headerMenu'); -%>
		<div>
			<h4 style="margin: 20px 40px;" >Dashboard</h4>
        <div style="margin: 20px 40px;" class="card-columns">
			<div class="card border-primary mb-3" style="max-width: 18rem;">
				<div class="card-header"><button class="btn btn-primary" type="submit">Faculties</button></div>
				<div class="card-body text-primary">
				  <h5 class="card-title"><%= faculty %> Faculties Registered!</h5>
				  <!-- <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p> -->
				</div>
		      </div>
			  <div class="card border-secondary mb-3" style="max-width: 18rem; ">
				<div class="card-header"><button class="btn btn-secondary" type="submit">Internships</button></div>
				<div class="card-body text-secondary">
				  <h5 class="card-title">10 Internships Going!</h5>
				  <!-- <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p> -->
				</div>
			  </div>

			  <div class="card border-danger mb-3" style="max-width: 18rem;">
				<div class="card-header"><button class="btn btn-danger" type="submit">Students</button></div>
				<div class="card-body text-danger">
				  <h5 class="card-title"><%= student %> Students Registered!</h5>
				  <!-- <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p> -->
				</div>
			  </div>
			  <div class="card border-warning mb-3" style="max-width: 18rem;">
				<div class="card-header"><button class="btn btn-warning" type="submit">Courses</button></div>
				<div class="card-body text-warning">
				  <h5 class="card-title"><%= course %> Coursed Launched!</h5>
				  <!-- <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p> -->
				</div>
			  </div>
			  <div class="card border-info mb-3" style="max-width: 18rem;">
				<div class="card-header"><button class="btn btn-info" type="submit">Applications</button></div>
				<div class="card-body text-info">
				  <h5 class="card-title"><%= application %> Applications!</h5>
				</div>
			  </div>

			  <div class="card border-dark mb-3" style="max-width: 18rem;">
				<div class="card-header"><button class="btn btn-dark" type="submit">Enquiries</button></div>
				<div class="card-body text-dark">
				  <h5 class="card-title"><%= query %> Enquiries!</h5>
				  <!-- <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p> -->
				</div>
			  </div>
		</div>
		</div>
