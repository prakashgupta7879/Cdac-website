						<% newListItems.forEach(function(item){ %>
							<li>
								<div class="icheck-primary d-inline ml-2">
								  <input class="strikethrough" type="checkbox" value="" name="todo3" id="todoCheck3">
								  <span style="margin-left: 10px;" class="text"><%= item.name %></span>
								</div>
								<small class="badge badge-warning"><i class="far fa-clock"></i>New</small>
							</li>
						<% }) %>
						

					  </ul>
					</div>
					<!-- /.card-body -->
					<form action="/adminDash" method="post">
						
						<div class="card-footer clearfix">
							<input type="text" name="newToDo" placeholder="Add Something...">
							<button type="submit" class="btn btn-primary float-right"><i class="fas fa-plus"></i> Add item</button>
						  </div>
					</form>
					
				  </div>
				  
